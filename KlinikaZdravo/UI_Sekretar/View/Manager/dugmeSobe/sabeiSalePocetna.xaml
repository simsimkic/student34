        </Button>

        <TextBlock HorizontalAlignment="Left" Margin="278,230,0,0" TextWrapping="Wrap" Text="Bolnicko lijecenje" VerticalAlignment="Top" FontSize="15" RenderTransformOrigin="0.951,0.143" Height="24"/>
        <TextBlock HorizontalAlignment="Left" Margin="464,230,0,0" TextWrapping="Wrap" Text="Sale i ordinacije" VerticalAlignment="Top" FontSize="15" RenderTransformOrigin="0.648,0.43" Height="24"/>
        <Button Content="Pocetna strana" HorizontalAlignment="Left" Margin="727,314,0,0" VerticalAlignment="Top" Width="93" Click="pocetnaa" Height="24"/>
        <Button Content="Odjava" HorizontalAlignment="Left" Margin="727,371,0,0" VerticalAlignment="Top" Width="93"  Height="23" Click="odjavaa"/>

    </Grid>
