        xmlns:local="clr-namespace:Aplikacija.dugmeSobe"
        mc:Ignorable="d"
        Title="sabeiSalePocetna" Height="486.667" Width="890" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,1.8,-0.4">
        <Rectangle Name="tema" HorizontalAlignment="Left" Height="457" Margin="0,0,-0.6,0" Stroke="White" VerticalAlignment="Top" Width="882" RenderTransformOrigin="0.084,1.006"/>
        <Image Margin="545,25,202.4,403.2" RenderTransformOrigin="0.623,0.615">
            <Image.Source>
                <BitmapImage DecodePixelWidth="300" UriSource="/icons/klinika.png" />
            </Image.Source>
        </Image>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="KLINIKA ZDRAVLJE" VerticalAlignment="Top" Margin="638,25,0,0" Width="205" Height="39" FontSize="20" FontWeight="Bold" FontStyle="Italic"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Upravnik" VerticalAlignment="Top" Margin="51,28,0,0" Width="205" Height="39" FontSize="20" FontWeight="Bold" FontStyle="Italic" Grid.ColumnSpan="2"/>

        <Button Margin="278,181,505.4,234.2" Click="bolnickoPocetna" >
